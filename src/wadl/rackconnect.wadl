	<!-- List all networks -->
	<!--*******************************************************-->
	<method 
		id="listAllNetworks" 
		name="GET">
		<wadl:doc 
			title="List all cloud networks"
			xml:lang="EN" 
			xmlns="http://docbook.org/ns/docbook">
			<para role="shortdesc">
				List details for all cloud networks. 
			</para>
			<para>This operation 
				lists details for 
				all cloud networks 
				available to <code>tenant_id</code>.
			</para>
		</wadl:doc>
		<!-- no request body = no sample request -->
		<response 
			status="200" 
			xmlns="http://wadl.dev.java.net/2009/02">
			<doc 
				title="OK" 
				xml:lang="EN">Success.</doc>
			<representation 
				mediaType="application/json">
				<wadl:doc 
					xmlns="http://docbook.org/ns/docbook"
					xml:lang="EN">
					<xsdxt:code
						href="../wadl/samples/rc3-allNetworks-response.json"
					/>
				</wadl:doc>
			</representation>
		</response>
	</method>
