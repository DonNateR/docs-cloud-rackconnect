    			style="template"
    			required="true"
    			type="xsd:string">
    			<wadl:doc 
    				xmlns="http://docbook.org/ns/docbook">
    				<para>
    					Specifies the unique identifier of a load balancer pool.
    				</para>
    			</wadl:doc>
    		</param>
    		<method href="#LBPools-list1" />
    	</resource>
    		<resource 
    			id="nodes" 
    			path="nodes">
    			<method href="#LBPools-nodes-listall" />
    			<method href="#LBPools-nodes-add1" />
    		</resource>
    			<resource 
    				id="details" 
    				path="details">
