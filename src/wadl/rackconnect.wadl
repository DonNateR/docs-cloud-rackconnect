    		<wadl:doc 
    			xmlns="http://docbook.org/ns/docbook">
    			<para>
    				Specifies the UUID of a cloud server.
    			</para>
    		</wadl:doc>
    	</param>
    	<method 
    		href="#publicIPs-cloudServers-list1" />
