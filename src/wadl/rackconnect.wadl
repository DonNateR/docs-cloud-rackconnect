  	<!-- endpoint = 
  	https://<region>.rackconnect.api.rackspacecloud.com/v3/tenantId/
    per 
    http://www.rackspace.com/knowledge_center/article/
  		rackconnect-v30-api-add-and-remove-public-ip-addresses
  	-->
<resources base="https://dfw.rackconnect.api.rackspacecloud.com/">
    <resource 
    	id="version" 
    	path="v3">
        <resource 
        	id="tenant_id" 
        	path="{tenant_id}">
	        <param name="tenant_id" 
	        	style="template"
	        	required="true" 
	        	type="xsd:string">
	            <wadl:doc 
	            	xmlns="http://docbook.org/ns/docbook">
	                 <para>
	      				The unique identifier of the tenant or account
	     			    in a multi-tenancy cloud.
	    			 </para>
	  			</wadl:doc>
			</param>
    <!-- ========================================= -->
    <!-- resources for *cloud networks* begin here -->    	
	<resource 
		id="cloud_networks" 
		path="cloud_networks">
	  <wadl:doc 
	  	title="Cloud networks"/>   
	  <method 
	  	href="#listAllNetworks" />
	  
		<resource 
			id="network_id" 
			path="{network_id}" >
	    	<param name="network_id" 
	    		style="template"
	    		required="true"
	    		type="xsd:string">
	     	<wadl:doc 
	     		xmlns="http://docbook.org/ns/docbook">
	      		<para>
					The unique identifier of a network.
	      		</para>
	       </wadl:doc>
		   </param>
	  	<method 
	  		href="#retrieve1Network" />
	  </resource>
		
	</resource>
    <!-- resources for *cloud networks* end here --> 
    <!-- ========================================= -->
    <!-- resources for *load balancer pools* begin here -->    	
    <resource 
    	id="load_balancer_pools" 
    	path="load_balancer_pools">
      <wadl:doc 
      	title="Load balancer pools"/>   
        		<method href="#listAllLBPools" />

    	<resource 
    		id="load_balancer_pool_id" 
    		path="{load_balancer_pool_id}">
    		<param name="load_balancer_pool_id" 
    			style="template"
    			required="true"
    			type="xsd:string">
    			<wadl:doc 
    				xmlns="http://docbook.org/ns/docbook">
    				<para>
    					The unique identifier of a load balancer pool.
    				</para>
    			</wadl:doc>
    		</param>
    		<method href="#summary1LBPool" />
    		<resource 
    			id="nodes" 
    			path="nodes">
    			<method href="#listNodes1LBPool" />
    			<method href="#addNode1LBPool" />
    		</resource>
    			<resource 
    				id="details" 
    				path="details">
    			<method href="#detail1LBPool" />
    		</resource>
    	</resource>

     </resource>
    <!-- resources for *load balancer pools* end here -->
    <!-- ========================================= -->
    <!-- resources for *public IPs* begin here -->    	
    <resource 
    	id="public_ips" 
    	path="public_ips">
      <wadl:doc title="Public IPs"/>   
    	<method href="#listAllPublicIPs" />
    	<method href="#add1PublicIP" />
    	<resource
    		id="cloud_server_UUID" 
    		path="cloud_server_id">
    	<param 
    		name="cloud_server_UUID" 
    		style="query"
    		type="xsd:string">
    		<wadl:doc 
    			xmlns="http://docbook.org/ns/docbook">
    			<para>
    				The UUID of a cloud server.
    			</para>
    		</wadl:doc>
    	</param>
    	<method 
    		href="#listPublicIPs1server" />
    	</resource>
    	
    	<resource 
    		id="public_IP_id" 
    		path="{public_IP_id}">
    		<param name="public_IP_id" 
    			style="template"
    			required="true"
    			type="xsd:string">
    			<wadl:doc 
    				xmlns="http://docbook.org/ns/docbook">
    				<para>
    					The unique identifier of a public IP.
    				</para>
    			</wadl:doc>
    		</param>
    		<method href="#retrieve1publicIP" />
    		<method href="#delete1publicIP" />
    	</resource>    	
    	
    </resource>
    <!-- resources for *public IPs* end here -->
    <!-- ========================================= -->
      </resource>
    </resource>
