	        	style="template"
	        	required="true" 
	        	type="xsd:string">
	            <wadl:doc 
	            	xmlns="http://docbook.org/ns/docbook">
	                 <para>
	      				Specifies the unique identifier of the tenant or account
	     			    in a multi-tenancy cloud.
	    			 </para>
	  			</wadl:doc>
			</param>
    <!-- ========================================= -->
    <!-- resources for *cloud networks* begin here -->    	
	<resource 
		id="cloud_networks" 
		path="cloud_networks">
	  <wadl:doc 
	  	title="Cloud networks"/>   
	  <method 
	  	href="#cloudNetworks-listall" />
	  
		<resource 
			id="network_id" 
			path="{network_id}" >
	    	<param name="network_id" 
	    		style="template"
	    		required="true"
	    		type="xsd:string">
	     	<wadl:doc 
	     		xmlns="http://docbook.org/ns/docbook">
	      		<para>
					Specifies the unique identifier of a network.
	      		</para>
	       </wadl:doc>
		   </param>
	  	<method 
	  		href="#cloudNetworks-list1" />
	  </resource>
		
	</resource>
    <!-- resources for *cloud networks* end here --> 
    <!-- ========================================= -->
    <!-- resources for *load balancer pools* begin here -->    	
    <resource 
    	id="load_balancer_pools" 
    	path="load_balancer_pools">
      <wadl:doc 
      	title="Load balancer pools"/>   
